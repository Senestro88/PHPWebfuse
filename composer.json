{
  "name": "senestro/phpwebfuse",
  "description": "An almost complete php web fuse",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "version": "6.2.1",
  "keywords": [
    "phpwebfuse",
    "webfuse",
    "fuse"
  ],
  "authors": [{
    "name": "John Habila",
    "email": "Senestro88@gmail.com",
    "role": "Developer"
  }],
  "support": {
    "email": "Senestro88@gmail.com"
  },
  "prefer-stable": true,
  "autoload": {
    "files": [
      "autoload.php"
    ]
  },
  "lock": false,
  "readme": "README.md",
  "time": "2024-01-10",
  "archive": {
    "name": "PHPWebfuse"
  },
  "require": {
    "php": ">=8.0",
    "ext-zlib": "*",
    "ext-ftp": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-bz2": "*",
    "ext-fileinfo": "*",
    "james-heinrich/getid3": "^2.0@beta",
    "rakibtg/sleekdb": "^2.15",
    "symfony/process": "^7.1",
    "phpmailer/phpmailer": "^6.9",
    "pear/archive_tar": "^1.5",
    "giggsey/libphonenumber-for-php": "^8.13",
    "mobiledetect/mobiledetectlib": "^4.8",
    "guzzlehttp/guzzle": "^7.0",
    "nelexa/zip": "^4.0",
    "phpseclib/phpseclib": "^3.0",
    "symfony/string": "^7.1",
    "symfony/filesystem": "^7.1",
    "vlucas/phpdotenv": "^5.6",
    "maennchen/zipstream-php": "^3.1",
    "nesbot/carbon": "^3.8",
    "chillerlan/php-qrcode": "^5.0",
    "chillerlan/php-authenticator": "^5.2"
  },
  "suggest": {
    "ext-libsodium": "SSH2/SFTP can make use of some algorithms provided by the libsodium-php extension.",
    "ext-mcrypt": "Required to speed up a few other cryptographic operations.",
    "ext-openssl": "Required to support encrypt zip entries or use ext-mcrypt",
    "ext-bz2": "Required to support BZIP2 compression",
    "ext-fileinfo": "Required to get mime-type file"
  }
}
