{
    "name": "senestro/phpwebfuse",
    "description": "An almost complete php web fuse",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "version": "4.1.0",
    "authors": [
        {
            "name": "John Habila",
            "email": "Senestro88@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "Senestro88@gmail.com"
    },
    "prefer-stable": true,
    "autoload": {
        "files": [
            "autoload.php"
        ]
    },
    "lock": false,
    "readme": "README.md",
    "time": "2024-01-10",
    "archive": {
        "name": "PHPWebfuse"
    },
    "require": {
        "php": ">=8.0",
        "ext-zlib": "*",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "james-heinrich/getid3": "^2.0@beta",
        "rakibtg/sleekdb": "^2.15",
        "symfony/process": "^7.1",
        "phpmailer/phpmailer": "^6.9",
        "pear/archive_tar": "^1.5",
        "giggsey/libphonenumber-for-php": "^8.13",
        "mobiledetect/mobiledetectlib": "^4.8",
        "nelexa/zip": "^4.0",
        "phpseclib/phpseclib": "^3.0",
        "symfony/string": "^7.1",
        "symfony/filesystem": "^7.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "suggest": {
        "ext-libsodium": "SSH2/SFTP can make use of some algorithms provided by the libsodium-php extension.",
        "ext-mcrypt": "Install the Mcrypt extension in order to speed up a few other cryptographic operations."
    }
}
